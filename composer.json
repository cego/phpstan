{
    "name": "cego/phpstan",
    "description": "Package for a common CEGO PhpStan extension used company wide",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Niki Ewald Zakariassen",
            "email": "niza@cego.dk"
        }
    ],
    "autoload-dev": {
        "psr-4": {
            "Test\\": "test/"
        }
    },
    "require": {
        "php": "^8.3",
        "phpstan/phpstan": "^2.0",
        "larastan/larastan": "^3.0",
        "spatie/laravel-data": "^3.0|^4.0",
        "phpstan/phpstan-strict-rules": "^2.0",
        "shipmonk/phpstan-baseline-per-identifier": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.0",
        "cego/php-cs-fixer": "^2.0"
    }
}
